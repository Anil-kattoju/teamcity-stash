<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
        default-autowire="constructor"
        >

  <bean class="mendhak.teamcity.github.ui.UpdateChangePathsImpl"/>
  <bean class="mendhak.teamcity.github.ui.UpdateChangeStatusFeature"/>
  <bean class="mendhak.teamcity.github.ui.UpdateChanesStatusController"/>

  <bean class="mendhak.teamcity.github.ChangeStatusListener"/>
  <bean class="mendhak.teamcity.github.ChangeStatusUpdater"/>

  <bean class="mendhak.teamcity.github.api.impl.GitHubApiFactoryImpl"/>
  <bean class="mendhak.teamcity.github.api.impl.HttpClientWrapperImpl" destroy-method="dispose"/>
</beans>
